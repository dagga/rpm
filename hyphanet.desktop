[Desktop Entry]
Name=Hyphanet
Comment=Open Hyphanet
Exec=xdg-open http://127.0.0.1:8888/
Icon=hyphanet
Terminal=false
Type=Application
Categories=Network;P2P;
Actions=start;stop;
[Desktop Action start]
Name=Start Hyphanet
Exec=pkexec systemctl start hyphanet.service

[Desktop Action stop]
Name=Stop Hyphanet
Exec=pkexec systemctl stop hyphanet.service
